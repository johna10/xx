<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="op_ticket_model_action" model="ir.actions.act_window">
         <field name="name">Op ticket details</field>
         <field name="res_model">op.ticket</field>
         <field name="view_mode">list,form</field>
    </record>

    <!--List view-->
    <record id="estate_list_view" model="ir.ui.view">
        <field name="name">list</field>
        <field name="model">op.ticket</field>
        <field name="arch" type="xml">
            <list string="Tests">
                <field name="serial_no"/>
                <field name="patient_name"/>
                <field name="age"/>
                <field name="patient_gender"/>
                <field name="doctor_name"/>
                <field name="date_and_time"/>
                <field name="fee"/>
            </list>
        </field>
    </record>
</odoo>