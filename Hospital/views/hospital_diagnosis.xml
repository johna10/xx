<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="diagnosis_model_action" model="ir.actions.act_window">
         <field name="name">Diagnosis Details</field>
         <field name="res_model">hospital.diagnosis</field>
         <field name="view_mode">list,form</field>
    </record>
    <!--List view-->
    <record id="diagonosis_list_view" model="ir.ui.view">
        <field name="name">list</field>
        <field name="model">hospital.diagnosis</field>
        <field name="arch" type="xml">
            <list string="Tests">
                <field name="medicine"/>
                <field name="dose"/>
                <field name="quantity"/>
            </list>
        </field>
    </record>
</odoo>