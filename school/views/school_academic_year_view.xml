<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--List View-->
    <record id="school_school_academic_list_view" model="ir.ui.view">
        <field name="name">list</field>
        <field name="model">school.academic</field>
        <field name="arch" type="xml">
            <list string="Year" editable="top">
                <field name="name"/>
            </list>
        </field>
    </record>

    <!--Action-->
    <record id="school_academic_model_action" model="ir.actions.act_window">
         <field name="name">School Academic</field>
         <field name="res_model">school.academic</field>
         <field name="view_mode">list</field>
    </record>
</odoo>