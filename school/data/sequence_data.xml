<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="my_sequence_id_for_school_registration" model="ir.sequence">
            <field name="name">My Sequence for Student Registration</field>
            <field name="code">my_sequence_code_student_registration</field>
            <field name="prefix">STUDENT/REGISTERED-</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="my_sequence_id_for_school_admission" model="ir.sequence">
            <field name="name">My Sequence for Student Admission</field>
            <field name="code">my_sequence_code_student_admission</field>
            <field name="prefix">STUDENT/ADMISSION-</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>